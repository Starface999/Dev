<h1>Add a New Creature</h1>
<%= form_for :creature, url: creatures_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :image_url %><br>
    <%= f.text_area :image_url %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>